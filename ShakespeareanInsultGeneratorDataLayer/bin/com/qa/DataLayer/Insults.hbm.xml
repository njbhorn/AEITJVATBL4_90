<?xml version="1.0"?>
	<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.qa.DataLayer.Insult" table="insults" >
	
		<id name="id" type="int" access="field" >
			<column name="ID" />
			<generator class="identity" />
		</id>
		
		<property name="phrase1id" type="int" access="field" >
			<column name="Phrase1Id" not-null="true" />
		</property>
		
		<property name="phrase2id" type="int" access="field" >
			<column name="Phrase2Id" not-null="true" />
		</property>
				
		<property name="phrase3id"  type="int" access="field">
			<column name="Phrase3Id" not-null="true" />
		</property>
		
	</class>
</hibernate-mapping>